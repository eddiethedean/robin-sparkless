{
  "name": "from_utc_timestamp_test",
  "pyspark_version": "3.5.0",
  "input": {
    "schema": [
      { "name": "id", "type": "bigint" },
      { "name": "ts", "type": "timestamp" }
    ],
    "rows": [
      [1, "2024-06-15T12:00:00"],
      [2, "1970-01-01T00:00:00"]
    ]
  },
  "operations": [
    { "op": "withColumn", "column": "ts_tz", "expr": "from_utc_timestamp(col('ts'), 'America/Los_Angeles')" },
    { "op": "select", "columns": ["id", "ts_tz"] }
  ],
  "expected": {
    "schema": [
      { "name": "id", "type": "bigint" },
      { "name": "ts_tz", "type": "timestamp" }
    ],
    "rows": [
      [1, "2024-06-15T12:00:00.000000"],
      [2, "1970-01-01T00:00:00.000000"]
    ]
  }
}
