{
  "name": "with_weekday",
  "pyspark_version": "3.5.0",
  "input": {
    "schema": [
      { "name": "id", "type": "bigint" },
      { "name": "d", "type": "date" }
    ],
    "rows": [
      [1, "2024-01-01"],
      [2, "2024-01-02"],
      [3, "2024-01-07"]
    ]
  },
  "operations": [
    { "op": "withColumn", "column": "wd", "expr": "weekday(col('d'))" },
    { "op": "select", "columns": ["id", "wd"] }
  ],
  "expected": {
    "schema": [
      { "name": "id", "type": "bigint" },
      { "name": "wd", "type": "bigint" }
    ],
    "rows": [
      [1, 0],
      [2, 1],
      [3, 6]
    ]
  }
}
