{
  "name": "drop_columns",
  "pyspark_version": "3.5.0",
  "input": {
    "schema": [
      { "name": "id", "type": "bigint" },
      { "name": "age", "type": "bigint" },
      { "name": "name", "type": "string" }
    ],
    "rows": [
      [1, 25, "Alice"],
      [2, 30, "Bob"]
    ]
  },
  "operations": [
    { "op": "drop", "columns": ["age"] }
  ],
  "expected": {
    "schema": [
      { "name": "id", "type": "bigint" },
      { "name": "name", "type": "string" }
    ],
    "rows": [
      [1, "Alice"],
      [2, "Bob"]
    ]
  }
}
