      {"name": "age", "type": "bigint"},
      {"name": "name", "type": "string"}
    ],
    "rows": [
      [1, 25, "Alice"],
      [2, 30, "Bob"],
      [3, 35, "Charlie"]
    ]
  },
  "plan": [
    {
      "op": "filter",
