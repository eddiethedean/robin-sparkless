{
  "name": "with_isin",
  "pyspark_version": "3.5.0",
  "input": {
    "schema": [
      { "name": "id", "type": "bigint" },
      { "name": "x", "type": "int" }
    ],
    "rows": [
      [1, 1],
      [2, 2],
      [3, 5],
      [4, 3]
    ]
  },
  "operations": [
    { "op": "withColumn", "column": "in_set", "expr": "isin(col('x'), 1, 2, 3)" },
    { "op": "select", "columns": ["id", "x", "in_set"] },
    { "op": "orderBy", "columns": ["id"], "ascending": [true] }
  ],
  "expected": {
    "schema": [
      { "name": "id", "type": "bigint" },
      { "name": "x", "type": "int" },
      { "name": "in_set", "type": "boolean" }
    ],
    "rows": [
      [1, 1, true],
      [2, 2, true],
      [3, 5, false],
      [4, 3, true]
    ]
  }
}
